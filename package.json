{
  "name": "rehype-mermaid-cli",
  "version": "0.2.1",
  "description": "A Rehype plugin to render Mermaid diagrams in multiple themes using the Mermaid CLI.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "rehype",
    "mermaid",
    "diagram",
    "markdown",
    "html",
    "unified",
    "remark",
    "plugin"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "lint:fix": "tsc --noEmit",
    "check": "npm run lint && npm run test",
    "prepublishOnly": "npm run check && npm run build",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidteather/rehype-mermaid-cli.git"
  },
  "author": "David Teather <contact.davidteather@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidteather/rehype-mermaid-cli/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/davidteather/rehype-mermaid-cli#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.0",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "typescript": "^5.9.2",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^11.9.0",
    "hast": "^0.0.2",
    "hast-util-from-html": "^2.0.3",
    "hast-util-to-text": "^4.0.2",
    "unified": "^11.0.5",
    "unist-util-visit-parents": "^6.0.1"
  }
}
